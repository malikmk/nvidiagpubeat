{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": {
          "enabled": false
        }
      },
      "_meta": {
        "version": "6.0.0-alpha1"
      },
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "name": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            },
            "version": {
              "ignore_above": 1024,
              "index": "not_analyzed",
              "type": "string"
            }
          }
        },
        "gpu_bar1_free_mb": {
          "type": "float"
        },
        "gpu_bar1_total_mb": {
          "type": "float"
        },
        "gpu_bar1_used_mb": {
          "type": "float"
        },
        "gpu_frame_buffer_free_mb": {
          "type": "float"
        },
        "gpu_frame_buffer_total_mb": {
          "type": "float"
        },
        "gpu_frame_buffer_used_mb": {
          "type": "float"
        },
        "gpu_id": {
          "index": "analyzed",
          "norms": {
            "enabled": false
          },
          "type": "string"
        },
        "gpu_memory_utilization_pct": {
          "type": "float"
        },
        "gpu_processor_utilization_pct": {
          "type": "float"
        },
        "tags": {
          "ignore_above": 1024,
          "index": "not_analyzed",
          "type": "string"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "nvidiagpubeat-*"
}