              "ignore_above": 1024,
              "type": "keyword"
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "version": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "gpu": {
          "properties": {
            "bar1_free_mb": {
              "type": "float"
            },
            "bar1_total_mb": {
              "type": "float"
            },
            "bar1_used_mb": {
              "type": "float"
            },
            "frame_buffer_free_mb": {
              "type": "float"
            },
            "frame_buffer_total_mb": {
              "type": "float"
            },
            "frame_buffer_used_mb": {
              "type": "float"
            },
            "id": {
              "norms": false,
              "type": "text"
            },
            "memory_utilization_pct": {
              "type": "float"
            },
            "processor_utilization_pct": {
              "type": "float"
            }
          }
        },
        "tags": {
          "ignore_above": 1024,
          "type": "keyword"
        }
      }
    }
